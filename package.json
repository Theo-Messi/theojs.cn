    "dev": "vitepress dev",
    "build": "vitepress build",
    "preview": "vitepress preview",
    "prettier": "prettier . --write",
    "check": "prettier . --check"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@types/node": "^20.14.11",
    "lint-staged": "^15.2.7",
    "simple-git-hooks": "^2.11.1",
    "vitepress-carbon": "1.3.3"
  },
  "devDependencies": {
    "prettier": "3.3.3",
    "sass": "^1.77.8",
    "vitepress": "1.3.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "printWidth": 120,
    "trailingComma": "none"
